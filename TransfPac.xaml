<Window x:Class="stac.TransfPac"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:stac"
        mc:Ignorable="d"
        Title="Перевод пациента" Height="600" Width="800">
    <Grid Background="#F4F4F4">
        <ComboBox HorizontalAlignment="Center" Margin="0,36,0,0" VerticalAlignment="Top" Width="290" Background="#D9D9D9" Foreground="#747373" Height="30"/>
        <Button x:Name="ButtonAddDoc" FontSize="15" Content="Сохранить данные" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Foreground="White" Height="30" Width="104" Background="#34495E" />
        <Button x:Name="ButtonDelDoc" FontSize="15" Content="Отмена" HorizontalAlignment="Left" Margin="656,36,0,0" VerticalAlignment="Top" Foreground="White" Height="30" Width="104" Background="#34495E" Click="ButtonDelDoc_Click" />
        <ComboBox HorizontalAlignment="Center" Margin="0,91,0,0" VerticalAlignment="Top" Width="290" Background="#D9D9D9" Foreground="#747373" Height="30"/>
        <ComboBox HorizontalAlignment="Center" Margin="0,142,0,0" VerticalAlignment="Top" Width="290" Background="#D9D9D9" Foreground="#747373" Height="30"/>
    </Grid>
</Window>
